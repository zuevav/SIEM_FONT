# SIEM Frontend Environment Variables

# API Backend URL
VITE_API_URL=http://localhost:8000/api/v1

# WebSocket URL (must point to /ws/dashboard endpoint)
VITE_WS_URL=ws://localhost:8000/ws/dashboard

# Enable debug mode (shows console logs)
VITE_DEBUG=false

# App Name
VITE_APP_NAME=SIEM System

# App Version
VITE_APP_VERSION=0.9.0
